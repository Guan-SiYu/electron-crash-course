{
    "name": "shoppinglist",
    "version": "1.0.0",
    "description": "Shopping list desktop",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "package-mac": "electron-packager . --overwrite --platform=darwin  --arch=arm64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
        "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ShoppingList\"",
        "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/iconic.png --prune=true --out=release-builds"
    },
    "keywords": [],
    "author": "guansiyu",
    "license": "MIT",
    "devDependencies": {
        "electron": "^16.0.1",
        "electron-packager": "^15.4.0"
    }
}
